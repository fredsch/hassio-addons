{
  "name": "Bayrol Poolaccess MQTT",
  "description": "This Add-on allows you to connect to bayrol poolaccess server and publish data to your Home Assistant Mqtt broker.",
  "image" : "tdenolle/bayrol-poolaccess-mqtt",
  "url": "https://github.com/tdenolle/bayrol-poolaccess-mqtt.git",
  "version": "1.0.1",
  "slug": "bayrol-poolaccess-mqtt",
  "services": [
	"mqtt:need"
  ],
  "udev": true,
  "uart": true,
  "environment": {
	"HASSIO_ADDON": "true"
  },
  "arch": [
	"aarch64",
	"amd64",
	"armhf",
	"armv7",
	"i386"
  ],
  "panel_icon": "mdi:console",
  "panel_title": "Bayrol Poolaccess MQTT Bridge",
  "options": {},
  "schema": {
	"LOG_LEVEL": "list(DEBUG|INFO|ERROR)",
	"DEVICE_TOKEN": "str",
	"DEVICE_SERIAL": "str",
	"HASS_DISCOVERY_PREFIX": "str",
	"MQTT_BASE_TOPIC": "str",
	"MQTT_URL": "str",
	"MQTT_USER": "str?",
	"MQTT_PASSWORD": "password?"
  }
}
